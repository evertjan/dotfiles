PS1="\[\033[01;34m\]\u@\h\[\033[01;36m\] \w\[\033[00m\]\n\$ "
export PS1
TZ="Europe/Amsterdam"
export TZ
LS_COLORS='di=01;34'
export LS_COLORS
EDITOR=vi
export EDITOR
GPG_TTY=$(tty)
export GPG_TTY
alias ll='ls -l --color=auto'
alias lla='ls -la --color=auto'
alias llt='ls -latr --color=auto'
alias ls='ls --color=auto'
alias vi='vim'
set -o vi

if [ -f ~/.bashgit ]; then
    . ~/.bashgit
fi
