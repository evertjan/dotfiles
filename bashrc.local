PS1="\[\033[01;36m\]\u@\h\[\033[01;32m\] \w\[\033[01;36m\] \$\[\033[00m\] "
export PS1
LS_COLORS='di=01;36'
export LS_COLORS
EDITOR=vi
export EDITOR
GPG_TTY=$(tty)
export GPG_TTY
alias ll='ls -l --color=auto'
alias lla='ls -la --color=auto'
alias llt='ls -latr --color=auto'
alias ls='ls --color=auto'
alias vi='vim'
set -o vi
